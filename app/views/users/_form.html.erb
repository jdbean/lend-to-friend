<%= form_with model: @user do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
  <br><br/>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
  <br><br/>
  <%= f.label :phone %><br>
  <%= f.text_field :phone %>
  <br><br/>
  <%= f.label :street_address %><br>
  <%= f.text_field :street_address %>
  <br><br/>
  <%= f.label :city %><br>
  <%= f.text_field :city %>
  <br><br/>
  <%= f.label :state %><br>
  <%= f.text_field :state %>
  <br><br/>
  <%= f.label :zipcode %><br>
  <%= f.text_field :zipcode %>
  <br><br/>
  <%= f.label :avatar_url %><br>
  <%= f.text_field :avatar %>
  <br><br/>
  <%= f.submit %>
<% end %>
