<%= form_with model: @item do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
  <br></br>
  <%= f.label :genre %><br>
  <%= f.text_field :genre %>
  <br></br>
  <%= f.label :description %><br>
  <%= f.text_field :description %>
  <br></br>
  <%= f.label :image %><br>
  <%= f.text_field :image %>
  <br></br>
  <%= f.submit %>
<% end %>
